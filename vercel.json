{
    "version": 2,
    "builds": [
        {
            "src": "cmd/api/main.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "cmd/api/main.go"
        }
    ],
    "functions": {
        "cmd/api/main.go": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}